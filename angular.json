{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "packages",
  "projects": {
    "common-log": {
      "projectType": "library",
      "root": "packages/common/log",
      "sourceRoot": "packages/common/log/src",
      "prefix": "pf",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "packages/common/log/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/common/log/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "packages/common/log/tsconfig.spec.json"
          }
        }
      }
    }
  }
}
